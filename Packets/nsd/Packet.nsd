#version 1.0;
#namespace Packets;

message Packet
{
    PacketType Type;
    byte Version;
    byte[] Data;
}

enum PacketType
{
    Null            = 0;
    Chat            = 100;
    Text            = 101;
    Entity          = 200;
    LoginRequest    = 300;
    LoginResponse   = 301;
    Logout          = 302;
}